 SUBRP	rnd			;A0=Mask
	PUSH	a2
	move	@RAND,a1,L
	rl	a1,a1
	rl	a0,a1
	move	@HCOUNT,a2
	rl	a2,a1
	xor	a2,a1
	move	a1,@RAND,L
	PULL	a2
	and	a1,a0		;A0=Rnd #
	rets			;Pass CC


