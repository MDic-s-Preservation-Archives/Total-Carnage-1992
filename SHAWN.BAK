	.word	-80,0,-74,-30,-56,-56,-30,-74

	.word	0,-60,23,-55,42,-42,55,-23
	.word	60,0,55,23,42,42,23,55
	.word	0,60,-23,55,-42,42,-55,23
	.word	-60,0,-55,-23,-42,-42,-23,-55

	.word	0,-40,15,-37,28,-28,37,-15
	.word	40,0,37,15,28,28,15,37
	.word	0,40,-15,37,-28,28,-37,15
	.word	-40,0,-37,-15,-28,-28,-15,-37

